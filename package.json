{
  "name": "@framers/wunderland",
  "version": "0.2.0",
  "description": "AI-powered personal assistant framework built on AgentOS - adaptive agents with HEXACO personality, multi-channel communication, hierarchical inference, and human-in-the-loop security",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./security": {
      "import": "./dist/security/index.js",
      "types": "./dist/security/index.d.ts"
    },
    "./inference": {
      "import": "./dist/inference/index.js",
      "types": "./dist/inference/index.d.ts"
    },
    "./authorization": {
      "import": "./dist/authorization/index.js",
      "types": "./dist/authorization/index.d.ts"
    },
    "./social": {
      "import": "./dist/social/index.js",
      "types": "./dist/social/index.d.ts"
    },
    "./guardrails": {
      "import": "./dist/guardrails/CitizenModeGuardrail.js",
      "types": "./dist/guardrails/CitizenModeGuardrail.d.ts"
    },
    "./tools": {
      "import": "./dist/tools/SocialPostTool.js",
      "types": "./dist/tools/SocialPostTool.d.ts"
    }
  },
  "bin": {
    "wunderland": "bin/wunderland.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "vitest",
    "test:security": "vitest run --reporter=verbose src/security",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "assistant",
    "agent",
    "agentos",
    "chatbot",
    "telegram",
    "discord",
    "slack",
    "whatsapp",
    "personal-assistant",
    "ai-agent",
    "llm",
    "automation",
    "hexaco",
    "personality",
    "hitl",
    "human-in-the-loop"
  ],
  "author": {
    "name": "jdunnfive",
    "url": "https://github.com/jddunn"
  },
  "license": "MIT",
  "homepage": "https://github.com/jddunn/wunderland",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jddunn/wunderland.git"
  },
  "bugs": {
    "url": "https://github.com/jddunn/wunderland/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@framers/agentos": "^0.1.6",
    "playwright-core": "^1.58.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  }
}
