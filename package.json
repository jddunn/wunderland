{
  "name": "wunderland",
  "version": "0.30.0",
  "description": "AI-powered personal assistant framework built on AgentOS, forked from OpenClaw channels/integrations/bridges - adaptive agents with HEXACO personality, multi-channel communication, hierarchical inference, and human-in-the-loop security",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./tools": {
      "import": "./dist/tools/index.js",
      "types": "./dist/tools/index.d.ts"
    },
    "./advanced": {
      "import": "./dist/advanced/index.js",
      "types": "./dist/advanced/index.d.ts"
    },
    "./advanced/*": {
      "import": "./dist/advanced/*.js",
      "types": "./dist/advanced/*.d.ts"
    },
    "./discovery": {
      "import": "./dist/discovery/index.js",
      "types": "./dist/discovery/index.d.ts"
    },
    "./presets": {
      "import": "./dist/core/PresetLoader.js",
      "types": "./dist/core/PresetLoader.d.ts"
    },
    "./presets/agents/*/agent.config.json": {
      "import": "./presets/agents/*/agent.config.json"
    },
    "./presets/*.json": {
      "import": "./presets/*.json"
    }
  },
  "files": [
    "dist",
    "bin",
    "presets",
    "assets"
  ],
  "bin": {
    "wunderland": "bin/wunderland.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:security": "vitest run --reporter=verbose src/security",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "assistant",
    "agent",
    "agentos",
    "chatbot",
    "telegram",
    "discord",
    "slack",
    "whatsapp",
    "personal-assistant",
    "ai-agent",
    "llm",
    "automation",
    "hexaco",
    "personality",
    "hitl",
    "human-in-the-loop"
  ],
  "author": {
    "name": "jdunnfive",
    "url": "https://github.com/jddunn"
  },
  "license": "MIT",
  "homepage": "https://github.com/framersai/voice-chat-assistant/tree/master/packages/wunderland",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framersai/voice-chat-assistant.git",
    "directory": "packages/wunderland"
  },
  "bugs": {
    "url": "https://github.com/framersai/voice-chat-assistant/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "@framers/agentos": "workspace:*",
    "@framers/agentos-ext-cli-executor": "workspace:*",
    "@framers/agentos-ext-giphy": "workspace:*",
    "@framers/agentos-ext-image-search": "workspace:*",
    "@framers/agentos-ext-news-search": "workspace:*",
    "@framers/agentos-ext-voice-synthesis": "workspace:*",
    "@framers/agentos-ext-web-browser": "workspace:*",
    "@framers/agentos-ext-web-search": "workspace:*",
    "@framers/agentos-extensions-registry": "workspace:*",
    "@framers/sql-storage-adapter": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.201.1",
    "@opentelemetry/auto-instrumentations-node": "^0.59.0",
    "@opentelemetry/sdk-node": "^0.201.1",
    "ansi-to-html": "^0.7.2",
    "boxen": "^8.0.1",
    "cfonts": "^3.3.0",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "gradient-string": "^3.0.0",
    "minimatch": "^9.0.3",
    "nanospinner": "^1.2.2",
    "playwright-core": "^1.58.1",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "@framers/agentos-ext-skills": "workspace:*",
    "@framers/agentos-skills-registry": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "@vitest/coverage-v8": "^1.3.0",
    "graphology": "^0.26.0",
    "graphology-communities-louvain": "^2.0.2",
    "graphology-types": "^0.24.8",
    "hnswlib-node": "^3.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  }
}
